<h1>Listing trashes</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% @trashes.each do |trash| %>
  <tr>
    <td><%=h trash.name %></td>
    <td><%= link_to 'Show', trash %></td>
    <td><%= link_to 'Edit', edit_trash_path(trash) %></td>
    <td><%= link_to 'Destroy', trash, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New trash', new_trash_path %>