<h1>Editing symmetric_function_game</h1>

<% form_for(@symmetric_function_game) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :number_of_strategies %><br />
    <%= f.text_field :number_of_strategies %>
  </p>
  <p>
    <%= f.label :number_of_players %><br />
    <%= f.text_field :number_of_players %>
  </p>
  <p>
    <%= f.label :color %><br />
    <%= f.text_field :color %>
  </p>
  <p>
    <%= f.label :function %><br />
    <%= f.text_field :function %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @symmetric_function_game %> |
<%= link_to 'Back', symmetric_function_games_path %>